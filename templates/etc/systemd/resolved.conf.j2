[Resolve]
DNS={{ dns | join(' ') }}
{% if fallback_dns %}
FallbackDNS={{ fallback_dns | join(' ') }}
{% endif %}
{% if dnssec %}
DNSSEC={{ dnssec }}
{% endif %}
{% if ansible_local.systemd.version | int >= 239 %}
{% if dns_over_tls %}
DNSOverTLS={{ dns_over_tls }}
{% endif %}
{% endif %}
